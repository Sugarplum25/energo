{
    "extends": ["stylelint-config-standard", "stylelint-config-rational-order"],
    "plugins": [
        "stylelint-order"
    ],
    "rules": {
        "indentation": 2,
        "at-rule-no-unknown": null,
        "order/order": [
            "custom-properties",
            "declarations"
        ]
    }
}